# Kaggle API Configuration (if not using ~/.kaggle/kaggle.json)
# KAGGLE_USERNAME=your_kaggle_username
# KAGGLE_KEY=your_kaggle_api_key

# Optional: Custom Kaggle config directory
# KAGGLE_CONFIG_DIR=/path/to/kaggle/config

# Development settings
DEBUG=false
LOG_LEVEL=INFO

# Resource limits
MAX_MEMORY_GB=16
N_JOBS=-1

# GPU settings
USE_GPU=false
CUDA_VISIBLE_DEVICES=0

# Experiment tracking
TRACK_EXPERIMENTS=true
EXPERIMENT_NAME_PREFIX=kaggle_exp_

# Optional: External services
# MLFLOW_TRACKING_URI=http://localhost:5000
# WANDB_API_KEY=your_wandb_key